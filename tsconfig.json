{
    "compilerOptions": {
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "target": "ES2024",
        "jsx": "preserve",
        "strictFunctionTypes": true,
        "noEmit": true,
        "sourceMap": true,
        "resolveJsonModule": true,
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}